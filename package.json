{
  "name": "auto_ai_agent",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "description": "AI Car Diagnostics Application",
  "main": "client/src/index.js",
  "browser": {
    "./src": "./client/src"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "@babel/preset-react": "^7.24.1",
    "@babel/runtime": "^7.24.5",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^7.3.3",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.15.12",
    "@popperjs/core": "^2.11.8",
    "@rails/actioncable": "^7.1.3",
    "@rails/activestorage": "^7.1.3",
    "@rails/ujs": "^7.1.3",
    "@rails/webpacker": "^5.4.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "react_ujs": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/runtime-corejs3": "^7.24.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^2.0.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "react-refresh": "^0.14.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development --port 3035",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/client/src/$1",
      "^@components/(.*)$": "<rootDir>/client/src/components/$1",
      "^@pages/(.*)$": "<rootDir>/client/src/pages/$1",
      "^@utils/(.*)$": "<rootDir>/client/src/utils/$1",
      "^@styles/(.*)$": "<rootDir>/client/src/styles/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "client/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/client/src/setupTests.js"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "resolutions": {
    "webpack": "^5.91.0"
  }
}
